    "license": "MIT",
    "dependencies": {
        "apollo-server-express": "^2.10.1",
        "bcrypt": "^4.0.0",
        "compression": "^1.7.4",
        "cors": "^2.8.5",
        "dotenv": "^8.2.0",
        "express": "^4.17.3",
        "graphql": "^14.6.0",
        "graphql-import-node": "0.0.4",
        "graphql-playground-middleware-express": "^1.7.12",
        "graphql-tools": "^4.0.7",
        "http": "0.0.0",
        "jsonwebtoken": "^8.5.1",
        "mongodb": "^3.5.4",
        "ncp": "^2.0.0",
        "typescript": "^3.8.2"
    },
    "devDependencies": {
        "@types/bcrypt": "^3.0.0",
        "@types/compression": "^1.7.0",
        "@types/cors": "^2.8.6",
        "@types/express": "^4.17.2",
        "@types/graphql": "^14.5.0",
        "@types/jsonwebtoken": "^8.3.7",
        "@types/mongodb": "^3.3.16",
        "@types/node": "^13.7.6",
        "ts-node": "^8.6.2"
    }
